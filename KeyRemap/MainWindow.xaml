<Window x:Class="KeyRemap.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KeyRemap"
        mc:Ignorable="d"
        Name="KeyRemapWindow"
        Visibility="Visible"
        WindowStyle="None"
        WindowStartupLocation="CenterScreen"
        ResizeMode="NoResize"
        AllowsTransparency="True"
        Background="Transparent"
        Title="MKeyRemap" Height="540" Width="680" Loaded="KeyRemapWindow_Loaded">
    <Border CornerRadius="14" BorderThickness="1" BorderBrush="#FF292C31" Name="Container" Background="#FF16191E">
        <Frame x:Name="frame" NavigationUIVisibility="Hidden"/>
    </Border>
</Window>
