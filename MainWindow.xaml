<Window x:Class="KeyRemap.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KeyRemap"
        xmlns:viewmodels="clr-namespace:KeyRemap.ViewModels"
        xmlns:views="clr-namespace:KeyRemap.Views"
        mc:Ignorable="d"
        Name="KeyRemapWindow"
        Visibility="Visible"
        WindowStyle="None"
        WindowStartupLocation="CenterScreen"
        ResizeMode="NoResize"
        Title="MainWindow" Height="520" Width="660" Background="#FF16191E" Loaded="KeyRemapWindow_Loaded">
    <Canvas Name="Container">
        <Frame x:Name="frame" NavigationUIVisibility="Hidden"/>
    </Canvas>
</Window>
